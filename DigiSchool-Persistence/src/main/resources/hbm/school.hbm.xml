<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.digi.data.School" table="dgs_schl">
  	<id name="schoolId" type="string">
  		<column name="schl_id" />
  		<generator class="assigned"></generator>
  	</id>
  	<property name="schoolName" type="string">
  		<column name="schl_nm" />
  	</property>
  	<property name="address" type="string">
  		<column name="schl_adrs" />
  	</property>
  	<property name="contactDetails" type="string">
  		<column name="schl_cnt_dtl" />
  	</property>
  	<property name="status" type="string">
  		<column name="schl_st" />
  	</property>
  	<property name="createdOn" type="java.util.Date">
  		<column name="schl_crt_on" />
  	</property>
  	<property name="modifiedOn" type="java.util.Date">
  		<column name="schl_mod_on" />
  	</property>
  	<property name="modifiedBy" type="string">
  		<column name="schl_mod_by" />
  	</property>
  	<property name="logoId" type="string">
  		<column name="schl_logo_id" />
  	</property>
  </class>  
 </hibernate-mapping>